The following functions return well known graphs. Most of them can be
found in Brandstadt, Le and Spinrad.

\Declaration{DiscreteGraph}

4-Discrete Graph
\enskip
\vcenter{\xymatrix{
   {\bullet} & {\bullet} \\
   {\bullet} & {\bullet} 
}}

\Declaration{CompleteGraph}

4-Complete Graph
\enskip
\vcenter{\xymatrix{
   {\bullet} \ar@{-}[r] \ar@{-}[d] & {\bullet} \ar@{-}[d] \ar@{-}[dl]\\
   {\bullet} \ar@{-}[r] & {\bullet} \ar@{-}[ul]
}}

%%--
%%--
%%--4-Path Graph
%%--\xymatrix{
%%--   {\bullet} \ar@{-}[r] & {\bullet} \ar@{-}[r] &
%%--   {\bullet} \ar@{-}[r] & {\bullet} 
%%--}  

\Declaration{CycleGraph}
5-Cycle Graph
\xy /r10mm/:
\xypolygon5{@{*}}
\endxy

\Declaration{CubeGraph}
3-Cube Graph
\objectmargin={1pt}
\xygraph{
 \bullet-[r]\bullet-[d]\bullet-[l]\bullet-[u],
  -[ur(.5)]\bullet-[r]\bullet-[d]\bullet-[l]\bullet-[u],
  [r]-[ur(.5)],
  [r][d]-[ur(.5)],
  [r][d][l]-[ur(.5)],
}

\Declaration{OctahedralGraph}
3-Octahedral Graph
\xy /r10mm/:
{\xypolygon4{~:{:(.5,.5)::}@{*}}}
\endxy

\Declaration{JohnsonGraph}


\Declaration{CompleteBipartiteGraph}
2,3-Complete Bipartite Graph
\xymatrix{
   {\bullet} \ar@{-}[r] \ar@{-}[dr] \ar@{-}[ddr] & {\bullet} \\
   {\bullet} \ar@{-}[r]  \ar@{-}[ur] \ar@{-}[dr]& {\bullet} \\
    & {\bullet} 
}  

\Declaration{CompleteMultipartiteGraph}

\Declaration{RandomGraph}
5-Random Graph

\Declaration{WheelGraph}
Wheel Graph of Order 5
\xy /r10mm/:,
\drop{\bullet}%\xypolygon5{~<{-}\bullet}
\endxy

\Declaration{FanGraph}
4-Fan Graph
\objectmargin={1pt}
\xygraph{
 \bullet (-[u]\bullet-[r(.6)]-[dr(.4)]-?
   ,[u][r(.6)]\bullet-?
   ,-[r]\bullet-[u(.6)]\bullet-?
}

\Declaration{SunGraph}
4-Sun Graph
\objectmargin={1pt}
\xygraph{
 \bullet-[dr]\bullet-[dl]\bullet-[ul]\bullet-[ur],
 [dr(.5)]\bullet-[d]\bullet-[l]\bullet-[u]\bullet-[r],
 [dr(.5)]-[dl][r]-[ul]
}


\Declaration{SpikyGraph}
3-Spiky Graph
\objectmargin={1pt}
\xygraph{
 \bullet,-[r(.5)]\bullet-[dr(.5)]\bullet-[u(.5)]\bullet-[l(.5)]\bullet
  -[dl(.5)]\bullet-[r],
  -[d(.5)]-[dr(.5)]\bullet-[ur(.5)]
}

\Declaration{TrivialGraph}
Trivial Graph
\xygraph{
 \bullet
}


\Declaration{DiamondGraph}
Diamond Graph
\objectmargin={1pt}
\xygraph{
 \bullet,-[dl(.5)]\bullet-[dr(.5)]\bullet-[ur(.5)]\bullet-[l]\bullet,
  -[dr(.5)]\bullet
}

\Declaration{ClawGraph}
Claw Graph
\objectmargin={1pt}
\xygraph{
 \bullet,-[u(.5)]\bullet,-[dl(.5)]\bullet,-[dr(.5)]\bullet
}

\Declaration{PawGraph}
Paw Graph
\objectmargin={1pt}
\xygraph{
 \bullet,-[d(.5)]\bullet-[dl(.5)]\bullet-[r]\bullet-[ul(.5)]
}


\Declaration{HouseGraph}
House Graph
\objectmargin={1pt}
\xygraph{
 \bullet,-[dl(.5)]\bullet-[d(.5)]\bullet-[r]\bullet-[u(.5)]\bullet-[l]\bullet,
  -[dr(.5)]\bullet
}

\Declaration{BullGraph}
Bull Graph
\objectmargin={1pt}
\xygraph{
 \bullet,-[ul(.5)]\bullet-[u(.5)]\bullet,
  -[ur(.5)]\bullet-[u(.5)]\bullet,
  [ur(.5)]-[l]\bullet
}

\Declaration{AntennaGraph}
Antenna Graph
\objectmargin={1pt}
\xygraph{
 \bullet,-[dl(.5)]\bullet-[d(.5)]\bullet-[r]\bullet-[u(.5)]\bullet-[l]\bullet,
  -[dr(.5)]\bullet,
  -[u(.5)]\bullet
}

\Declaration{KiteGraph}
Kite Graph
\objectmargin={1pt}
\xygraph{
 \bullet,-[l(.5)]\bullet,
 -[dr(.5)]\bullet-[ur(.5)]\bullet-[ul(.5)]\bullet-[dl(.5)],
 [dr(.5)]-[u]
}

\Declaration{Tetrahedron}
Tetrahedron
\xygraph{ %/r10mm/:
 \bullet (-[u(.5)]\bullet,-[dr(.5)]\bullet,-[dl(.5)]\bullet),
 [u(.5)] ([d(.5)][dr(.5)]-?, [d(.5)][dl(.5)]-?),
 [dr(.5)]-[l]
}


\Declaration{Octahedron}

\Declaration{Cube}
Cube Graph
\objectmargin={1pt}
\xygraph{
 \bullet-[r]\bullet-[d]\bullet-[l]\bullet-[u],
  -[ur(.5)]\bullet-[r]\bullet-[d]\bullet-[l]\bullet-[u],
  [r]-[ur(.5)],
  [r][d]-[ur(.5)],
  [r][d][l]-[ur(.5)],
}

\Declaration{Icosahedron}
\Declaration{Dodecahedron}

%ar@{-}[r] \ar@{-}[dar@{-}[r] \ar@{-}[d]
%ar@{-}[r] \ar@{-}[d]

