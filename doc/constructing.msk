  [d(.5)][rd(.5)][r(.5)]-?,
  [d(.5)][rd(.5)][r(.5)][ru(.5)]-?,
  [d(.5)][rd(.5)][r(.5)][ru(.5)][u(.5)]-?,
 ),
 [d(.5)]
  ([rd(.5)]-?,
   [rd(.5)][r(.5)]-?,
   [rd(.5)][r(.5)][ru(.5)]-?,
   [rd(.5)][r(.5)][ru(.5)][u(.5)]-?,
